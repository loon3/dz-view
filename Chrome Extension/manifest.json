{
   "author": "Joe Looney",
   "description": "Parse Drop Zone txs",
   "icons": {
      "128": "icon-128.png",
      "48": "icon-48.png"
   },
   "content_scripts": [
    {
      "matches": ["https://chain.so/*"],
      "js": ["js/jquery.min.js", "js/find-dz-data.js", "js/bitcoinjs-min.js"]
    }
   ],
   "web_accessible_resources": [
        "js/jquery.min.js", 
        "js/bitcoinjs-min.js", 
        "js/find-dz-data.js",
        "js/test.js",
        "test.html"
   ],
   "manifest_version": 2,
   "name": "dz-view",
   "short_name": "dz-view",
   "version": "0.1.0"
}
